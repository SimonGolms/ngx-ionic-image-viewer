{
  "name": "ngx-ionic-image-viewer-workspace",
  "version": "0.0.0",
  "description": "The angular workspace to develop the ngx-ionic-image-viewer component",
  "keywords": [
    "ionic",
    "ionic4",
    "angular",
    "angular8",
    "ngx",
    "module",
    "component",
    "directive",
    "ion",
    "image",
    "img",
    "photo",
    "thumbnail",
    "view",
    "viewer",
    "show",
    "gallery",
    "zoom",
    "modal",
    "slider"
  ],
  "homepage": "https://github.com/simongolms/ngx-ionic-image-viewer.git",
